{
  "name": "doctext",
  "version": "0.0.1",
  "description": "Dynamic doctext insertion for NodeJS.",
  "private": true,
  "author": "",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc",
    "build:tsc": "pnpm build:tsc:esm && pnpm build:tsc:cjs",
    "build:tsc:esm": "tsc -p tsconfig.prod.json --module es2022 --outDir dist/esm --declarationDir dist/types",
    "build:tsc:cjs": "tsc -p tsconfig.prod.json --module commonjs --outDir dist/cjs --declaration false --declarationMap false",
    "clean": "rimraf dist",
    "check": "tsc -p . --noEmit",
    "lint": "eslint src --ext ts,tsx",
    "test": "NODE_ENV=test DATA_ENV=test jest"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "acorn-typescript": "^1.4.13",
    "fs-extra": "^11.3.2",
    "jest": "^30.2.0",
    "lodash": "^4.17.21",
    "ytil": "workspace:*"
  }
}