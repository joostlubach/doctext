{
  "name": "doctext",
  "version": "0.0.1",
  "type": "module",
  "description": "Dynamic doctext insertion for NodeJS.",
  "private": true,
  "author": "",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "check": "tsc -p . --noEmit",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "acorn": "^8.11.3",
    "acorn-typescript": "^1.4.13",
    "fs-extra": "^11.1.1",
    "ytil": "workspace:^"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  }
}